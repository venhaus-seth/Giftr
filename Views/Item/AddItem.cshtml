@{
    ViewData["Title"] = "Add Item";
}
@model MyViewModel
<div>
    @await Html.PartialAsync("_Navbar")

    @await Html.PartialAsync("_WishlistHeader", @Model.User)

    <div class=" d-flex justify-content-center">
        <div class=" d-flex  flex-column align-items-center mx-5">
            @await Html.PartialAsync("_Wishlist", @Model.User)
        </div>
        <div>
            @await Html.PartialAsync("_ItemForm", @Model.Item)
        </div>
    </div>
</div>